  <modelVersion>4.0.0</modelVersion>
  <groupId>org.gdgsrilanka.ecotrail.api</groupId>
  <artifactId>ecotrail-api</artifactId>
  <name>WSO2 MSF4J Microservice</name>
  <version>DAWN</version>
  <build>
    <resources>
      <resource>
        <directory>src/main/resources</directory>
        <excludes>
          <exclude>intro.html</exclude>
